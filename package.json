{
  "name": "lululemon-react-technical-exercise",
  "version": "0.0.1",
  "description": "lululemon React technical exercise",
  "engines": {
    "node": "10.12.0"
  },
  "scripts": {
    "webpack": "webpack --config config/webpack.config.production.js",
    "sass": "node-sass src/styles/index.scss build/index.css --output-style compressed",
    "build": "npm run sass && npm run webpack",
    "lint": "eslint src config",
    "dev": "webpack-cli serve --mode development --config config/webpack.config.dev.js",
    "start": "serve -s build"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.2.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^4.5.0",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass-loader": "^10.0.5",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  }
}
